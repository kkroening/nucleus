#!/usr/bin/env bash
find . \
    \( \
        -name .git -o \
        -name .ipynb_checkpoints -o \
        -name .tox -o \
        -name .venv\* -o \
        -name build -o \
        -name bandit-baseline.json -o \
        -name deploy -o \
        -name node_modules -o \
        -name venv -o \
        -name \*.bundle.js -o \
        -name \*.map -o \
        -name \*.min.js -o \
        -name \*.pyc -o \
        -name \*.pyo -o \
        -name \*.js -o \
        -name \*.sw\* -o \
        -name \*.vue -o \
        -name \*.sw\* -o \
        -name .pytest_cache -o \
        -name .lab_cache \
    \) -prune -o -type f -print0
